<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">
                List of Models
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="VehicleModel" asp-action="Upsert" class="btn btn-primary"> <i class="bi bi-plus-circle"></i> Create new vehicle model</a>
        </div>
    </div>

    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Make</th>
                <th>Model</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/vehicleModel.js"></script>
}

